{
  "name": "chonky-docs",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "docusaurus": "docusaurus",
    "release-version": "GIT_BRANCH_NAME=$(git rev-parse --abbrev-ref HEAD) docusaurus docs:version",
    "start": "GIT_BRANCH_NAME=$(git rev-parse --abbrev-ref HEAD) docusaurus start",
    "build": "GIT_BRANCH_NAME=$(git rev-parse --abbrev-ref HEAD) docusaurus build",
    "swizzle": "GIT_BRANCH_NAME=$(git rev-parse --abbrev-ref HEAD) docusaurus swizzle",
    "deploy": "docusaurus deploy",
    "clear": "docusaurus clear",
    "serve": "docusaurus serve",
    "write-translations": "docusaurus write-translations",
    "write-heading-ids": "docusaurus write-heading-ids",
    "build-s3-artifacts": "yarn run build",
    "format": "prettier --config ../../.prettierrc.js --ignore-path ../../.prettierignore . --write --list-different",
    "format-check": "prettier --config ../../.prettierrc.js --ignore-path ../../.prettierignore --check ."
  },
  "dependencies": {
    "@docusaurus/core": "2.0.0-beta.14",
    "@docusaurus/preset-classic": "2.0.0-beta.14",
    "@docusaurus/theme-live-codeblock": "^2.0.0-beta.14",
    "@mdx-js/react": "^1.6.21",
    "chonky": "^2.3.2",
    "chonky-icon-fontawesome": "^2.3.2",
    "clsx": "^1.1.1",
    "prism-react-renderer": "^1.2.1",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "webpack": "5.x"
  },
  "browserslist": {
    "production": [
      ">0.5%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "prettier": "^2.5.1"
  }
}
